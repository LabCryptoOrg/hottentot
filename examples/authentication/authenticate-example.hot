module naeem.hottentot.examples.auth {
  struct Credential {
    string username ord(1);
    string password ord(2);
  };
  struct User {
    string name ord(1);
    string family ord(2);
    uint8 age ord(3);
  };
  struct Token {
    string value ord(1);
  };
  stateless service Authenticate {
    Token authenticate(Credential credential);
    string f();
    int32 g();
    data h();
  };
};
